{
    "env": {
      "browser": true,
      "es6": true,
      "jest": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "extends": ["airbnb-base"],
    "rules": {
      "no-shadow": "off",
      "no-param-reassign": "off",
      "no-prototype-builtins":"off",
      "eol-last": "off",
      "no-console": "warn",
      "no-unused-vars": "warn",
      "no-use-before-define": "warn",
      "import/no-cycle": "off",
      "import/extensions": [ 1, {
        "js": "always", "json": "always"
      }]
    },
    "ignorePatterns": [
      "dist/",
      "build/",
      "server.js",
      "*.test.js"
    ]
  }